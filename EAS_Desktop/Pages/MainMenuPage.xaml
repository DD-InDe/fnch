<Page x:Class="EAS_Desktop.Pages.MainMenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:EAS_Desktop.Pages"
      mc:Ignorable="d"
      Title="Главное меню"
      d:DesignHeight="450"
      d:DesignWidth="800">

    <StackPanel Orientation="Vertical">
        <Button x:Name="ModulesButton"
        Click="ModulesButton_OnClick"
                Content="Адаптационные модули" />
        <Button x:Name="ConstructorButton"
        Click="ConstructorButton_OnClick"
                Content="Конструктор" />
        <Button x:Name="AnalyzeButton"
        Click="AnalyzeButton_OnClick"
                Content="Анализ адаптационных мероприятий" />
    </StackPanel>
</Page>